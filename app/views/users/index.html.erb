<% @users.each do |user| %>

  <div class="row">
    <div class="box">
      <div class="col-lg-12 header">
        <hr class="hr-post">
        <h2 class="intro-text text-center"><%= link_to user.username, action: 'show', id: user.id  %></h2>
        <hr class="hr-post">
      </div>
    </div>
  </div>
<% end %>
  <div class="pagination">
    <%= will_paginate @user, renderer: BootstrapPagination::Rails %>
  </div>