<% if @publish.contents.find_images.present? %>
  <div class="entry type-post style-thumbnail col-sm-3">
    <div class="view2">
      <figure class="entry-thumbnail">
        <%= image_tag @publish.theme.image.url(:medium) %>
      </figure>
      <div class="mask2">
        <h2>Images</h2>
      </div>
    </div>
  </div>
<% end %>

<% @publish.contents.find_images.each do |p| %>
<p><%= p.file_file_name %></p>
<% end %>
