<td colspan="6">
<%= form_for(object, url: object.new_record? ? admin_themes_path : admin_theme_path(object), remote: true, html: {class: "form-horizontal"}) do |f| %>
<p>Name: <%= f.text_field :name %></p>
<p>Image: <%= f.file_field :image %></p>
  <%= f.submit "Save", class: 'btn btn-primary' %>
  <button class="btn btn-link cancel">Cancel</button>


<% end %>
</td>
