<% unless smart_listing.empty? %>
  <table class="table table-striped">
    <thead>
        <th><%= smart_listing.sortable "Name", :name %></th>
        <th>Image</th>
    </thead>
    <tbody>
      <% smart_listing.collection.each do |o| %>
        <tr class="editable" data-id="<%= o.id %>">
          <%= smart_listing.render(object: o, partial: "admin/themes/theme", locals: {object: o}) %>
        </tr>
      <% end %>

      <%= smart_listing.item_new colspan: 6, link: new_admin_theme_path %>
    </tbody>
  </table>
  <%= smart_listing.paginate %>
  <%= smart_listing.pagination_per_page_links %>
<% end %>
